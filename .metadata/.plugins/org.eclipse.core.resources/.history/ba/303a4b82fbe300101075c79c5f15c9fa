#include "stm32f446xx.h"
#include "interrupts.h"

void tim2_init(void)
{
    RCC->APB1ENR |= (1<<0);

    TIM2->PSC = 84-1;
    TIM2->ARR = 1000-1;
    TIM2->DIER |= (1<<0);
    TIM2->CR1 |= (1<<0);

    NVIC_EnableIRQ(TIM2_IRQn);
}



